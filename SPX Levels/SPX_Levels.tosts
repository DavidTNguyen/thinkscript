# === Key Support & Resistance Levels ===
input level1 = 7000; # Year-end melt-up target
input level1_thickness = 2; # 1=normal, 2=(*), 3=(**)
input level2 = 6850; # Acceleration target
input level2_thickness = 1;
input level3 = 6795; # Post-breakout target (Â±4 pts)
